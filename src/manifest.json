{
  "manifest_version": 2,
  "name": "Provider for Google Calendar",
  "description": "Allows bidirectional access to Google Calendar",
  "version": "78.0pre1",
  "author": "Philipp Kewisch",
  "homepage_url": "https://addons.mozilla.org/thunderbird/addon/4631",
  "default_locale": "en",
  "applications": {
    "gecko": {
      "id": "{a62ef8ec-5fdc-40c2-873c-223b8a6925cc}",
      "strict_min_version": "78.1.1"
    }
  },
  "icons": {
    "256": "icon.png"
  },

  "background": {
    "scripts": ["background.js"]
  },

  "permissions": [
    "storage"
  ],

  "experiment_apis": {
    "gdata": {
      "schema": "api/schema.json",
      "parent": {
        "scopes": ["addon_parent"],
        "script": "api/gdata.js",
        "events": ["startup"],
        "paths": [
          ["gdata"]
        ]
      }
    }
  }
}
